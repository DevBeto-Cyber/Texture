  "paste_button@common.button": {
    "$button_label|default": "",
    "$paste_code|default": "",
    "$button_name": "paste_button",
    "$pressed_button_name": "",
    "$default_button_texture": "textures/ui/paste_button_hover",
    "$hover_button_texture": "textures/ui/paste_button",
    "$pressed_button_texture": "textures/ui/paste_button_pressed",
    "size": ["100%", 16],
    "controls": [
      {
        "paste_button_content@paste_button_content": {}
      },
      {
        "default": {
          "type": "image",
          "texture": "$default_button_texture"
        }
      },
      {
        "hover": {
          "type": "image",
          "texture": "$hover_button_texture"
        }
      },
      {
        "pressed": {
          "type": "image",
          "texture": "$pressed_button_texture"
        }
      },
      {
        "text_edit_box": {
          "type": "edit_box",
          "text_box_name": "#text_box_name",
          "max_length": 2147483647,
          "can_be_deselected": true,
          "text_control": "text_control",
          "always_listening": true,
          "focus_enabled": true,
          "size": ["100%sm", "100%sm"],
          "offset": [2, 0],
          "layer": 3,
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "button_mappings": [
            {
              "from_button_id": "button.menu_select",
              "to_button_id": "button.keyboard_toggle",
              "handle_select": true,
              "handle_deselect": false,
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.menu_select",
              "to_button_id": "button.keyboard_toggle",
              "handle_select": false,
              "handle_deselect": true,
              "mapping_type": "global",
              "consume_event": false
            },
            {
              "from_button_id": "button.menu_cancel",
              "to_button_id": "button.menu_select",
              "handle_select": false,
              "handle_deselect": true,
              "mapping_type": "global"
            }
          ],
          "controls": [
            {
              "text_control": {
                "type": "label",
                "layer": 9,
                "text": "#item_name",
                "visible": false,
                "property_bag": {
                  "#property_field": "#item_name"
                },
                "localize": false,
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "display_text",
                    "source_property_name": "#item_name",
                    "target_property_name": "#current_input_text"
                  },
                  {
                    "binding_type": "view",
                    "source_property_name": "(#current_input_text + $paste_code)",
                    "target_property_name": "#item_name"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },
